@page
@using Microsoft.AspNetCore.Mvc.Localization
@using SAO.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@using SAO.Web.Pages.Importadors
@using SAO.Importadors;
@using System.Globalization
@inject IHtmlLocalizer<SAOResource> L
@model CreateModalModel
@{
    Layout = null;
}

<form data-ajaxForm="true" asp-page="/Importadors/CreateModal" autocomplete="off">
    <abp-modal id="ImportadorCreateModal">
        <abp-modal-header title="@L["NewImportador"].Value"></abp-modal-header>

        <abp-modal-body>
                    
            <abp-input asp-for="Importador.NombreImportador"  />

                    
                
        </abp-modal-body>

        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</form>